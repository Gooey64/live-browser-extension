{
  "name": "windowextension",
  "displayName": "windowExtension",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
  "commands": [
  {
    "command": "liveBrowser.open",
    "title": "Live Browser: Open URL in Panel"
  },
  {
    "command": "liveBrowser.openFile",
    "title": "Live Browser: Open HTML File in Panel"
  }
  ],
  "viewsContainers": {
    "panel": [
      {
        "id": "live-browser-container",
        "title": "Live Browser",
        "icon": "$(browser)"
      }
    ]
  },
  "views": {
    "live-browser-container": [
      {
        "type": "webview",
        "id": "liveBrowser.view",
        "name": "Live Browser"
      }
    ]
  },
  "configuration": {
    "title": "Live Browser",
    "properties": {
      "liveBrowser.url": {
        "type": "string",
        "default": "https://example.com",
        "description": "URL to display in the Live Browser panel"
      }
    }
  }
},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
